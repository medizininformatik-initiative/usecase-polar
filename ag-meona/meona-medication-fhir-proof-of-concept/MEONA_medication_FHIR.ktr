    },
};

// Source: https://stackoverflow.com/a/10284006
function zip() {
    var args = [].slice.call(arguments);
    var longest = args.reduce(function(a,b){
        return a.length>b.length ? a : b
    }, []);

    return longest.map(function(_,i){
        return args.map(function(array){return array[i]})
    });
} // Source end

function push([name, amount, unit, is_active]) {
	template["ingredient"].push({
            "isActive": is_active,
            "itemCodeableConcept": {
				"text": name
            },
            "strength": {
				/*
                "denominator": {
                    //"code": "ml",
                    //"system": "http://unitsofmeasure.org",
                    //"unit": "ml",
                    
                },
				*/
                "numerator": {
                    //"code": "mg",
                    //"system": "http://unitsofmeasure.org",
                    //"unit": "mg",
                    "value": amount,
					"unit": unit
                }
            }
        })
}

function unique(value, index, self) {
  return indexOf(self, value) === index;
}

function indexOf(myArray, elementArray) {
	for(var i=0; i &lt; myArray.length; i++) {
		for(var j=0; i &lt; elementArray.length; j++) {
			if(myArray[i][j] === elementArray[j]) {
				return i;
		    } else {
				break
			}
		}
	}
}

zip(
	ingredient_name.split(", "),
	ingredient_amount.split(", "),
	ingredient_unit.split(", "),
	is_active_ingredient.split(", ")
).forEach(push);

var fhir = JSON.stringify(template);</jsScript_script>
